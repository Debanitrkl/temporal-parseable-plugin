# Parseable connection
PARSEABLE_URL=http://localhost:8000
PARSEABLE_USERNAME=admin
PARSEABLE_PASSWORD=admin

# Stream names (one per signal)
PARSEABLE_TRACES_STREAM=temporal-traces
PARSEABLE_LOGS_STREAM=temporal-logs
PARSEABLE_METRICS_STREAM=temporal-metrics

# Temporal connection
PARSEABLE_TEMPORAL_HOST=localhost:7233
PARSEABLE_TEMPORAL_NAMESPACE=default

# OpenTelemetry
PARSEABLE_SERVICE_NAME=temporal-worker

# Signal toggles
PARSEABLE_ENABLE_TRACES=true
PARSEABLE_ENABLE_LOGS=true
PARSEABLE_ENABLE_METRICS=true
